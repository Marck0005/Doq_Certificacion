<odoo>

    <!-- wizard_team_report form view -->
    <record id="wizard_team_report_view_form" model="ir.ui.view">
        <field name="name">wizard_team_report.view.form</field>
        <field name="model">wizard.team.report</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <!-- Add your fields here -->
                        <field name="initial_date"/>
                        <field name="final_date"/>
                        <field name="client_id"/>
                        <field name="team_id" invisible = "True"/>
                    </group>
                </sheet>
                <footer>
                    <button name="report_team" string="PDF-Report" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>





</odoo>